    "node-sass": "^4.11.0",
    "react-is": "^16.7.0",
    "rollup-plugin-postcss-modules": "^1.0.10",
    "styled-components": "^4.1.3"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "devDependencies": {
    "@svgr/rollup": "^2.4.1",
    "@types/jest": "^23.1.5",
    "@types/react": "^16.7.18",
